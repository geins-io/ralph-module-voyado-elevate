{
  "name": "@geins/ralph-module-voyado-elevate",
  "version": "5.1.2",
  "description": "Component library for Voyado Elevate integrations with Ralph Storefront",
  "author": {
    "name": "Geins"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js"
  ],
  "main": "lib/module.js",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "jest",
    "publish": "npm publish --access public",
    "test:unit": "jest --testRegex \"/test/unit/(.+)\\.test\\.js$\"",
    "test:system": "jest --testRegex \"/test/system/(.+)\\.test\\.js$\"",
    "test:coverage": "yarn test --coverage",
    "format": "prettier './**/*.{js,ts,json,vue}' --write",
    "lint": "eslint --fix --ext .js,.vue ."
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@nuxtjs/eslint-config": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^4.3.8",
    "jest": "^23.4.1",
    "lint-staged": "^10.5.4",
    "nuxt": "^2.4.0",
    "prettier": "^1.19.1",
    "puppeteer": "^1.6.1",
    "stylelint": "^13.13.1",
    "vue": "^2.7.14"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "@apptus/esales-api": "^2.4.3",
    "chalk": "^5.2.0",
    "consola": "^3.1.0",
    "semver": "^7.5.0"
  }
}
