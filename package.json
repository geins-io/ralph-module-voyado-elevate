{
  "name": "ralph-module-voyado-elevate",
  "version": "0.1.0",
  "description": "Boilerplate for creating a module for Geins PWA strorefront",
  "author": {
    "name": "arvidsson-geins"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js"
  ],
  "main": "lib/module.js",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "jest",
    "test:unit": "jest --testRegex \"/test/unit/(.+)\\.test\\.js$\"",
    "test:system": "jest --testRegex \"/test/system/(.+)\\.test\\.js$\"",
    "test:coverage": "yarn test --coverage",
    "format": "prettier './**/*.{js,ts,json,vue}' --write",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^4.0.0",
    "@apptus/esales-api": "^2.1.0",
    "jest": "^23.4.1",
    "nuxt": "^2.4.0",
    "puppeteer": "^1.6.1",
    "vue": "^2.7.14",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^1.19.1",
    "stylelint": "^13.13.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "consola": "^2.3.2"
  }
}
